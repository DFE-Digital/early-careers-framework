<div class="govuk-!-static-margin-bottom-9">
  <%= heading %>

  <% if empty? %>
    <p>No partnerships or relationships in the <%= cohort.start_year %> cohort.</p>

  <% else %>
    <% partnership_components.each do |pc| %>
      <%= pc %>
    <% end %>

    <% if relationship_components.any? %>
      <h3>Additional relationships</h3>

      <% relationship_components.each do |rc| %>
        <%= rc %>
      <% end %>
    <% end %>
  <% end %>
</div>
