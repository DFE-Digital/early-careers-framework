
<table class="govuk-table">
  <caption class="govuk-table__caption govuk-table__caption--l"><%=t "finance.other_fees.caption" %></caption>
  <thead class="govuk-table__head">
    <tr class="govuk-table__row">
      <th class="govuk-table__header"><%=t "finance.other_fees.fee"%></th>
      <th class="govuk-table__header"><%=t "finance.participants"%></th>
      <th class="govuk-table__header"><%=t "finance.per_participant"%></th>
      <th class="govuk-table__header"><%=t "finance.period"%></th>
    </tr>
  </thead>
  <tbody class="govuk-table__body">
    <%= render Finance::ECF::OtherFeeRow.with_collection(other_fees) %>
    <tr class="govuk-table__row">
      <th class="govuk-table__cell"><%=I18n.t("finance.subtotal") %></th>
      <td class="govuk-table__cell govuk-table__cell--numeric"><%= participants %></td>
      <td class="govuk-table__cell govuk-table__cell--numeric"></td>
      <td class="govuk-table__cell govuk-table__cell--numeric"><%= number_to_pounds subtotal %></td>
    </tr>
  </tbody>
</table>
