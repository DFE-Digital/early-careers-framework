<% content_for :before_content, govuk_back_link(text: 'Back', href: admin_suppliers_path) %>

<%= form_for @lead_provider, url: admin_supplier_path(@lead_provider) do |f| %>
  <%= f.govuk_text_field(
        :name,
        label: { text: 'Lead Provider name' })
  %>
  <%= f.govuk_submit "Update" %>
<% end %>

<%= govuk_link_to "Manage users", admin_lead_provider_users_path(@lead_provider) %>
