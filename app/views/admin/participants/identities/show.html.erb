<%= tag.h1 do %>
  <%= tag.span(@participant_profile.full_name, class: "govuk-caption-m") %>

  Identities
<% end %>

<%= render partial: "admin/participants/nav" %>

<% if @participant_identities.none? %>
  <p><%= @participant_profile.user.full_name %> has no identities.</p>
<% else %>
  <%= render Admin::Participants::Identities.new(identities: @participant_identities) %>
<% end %>
