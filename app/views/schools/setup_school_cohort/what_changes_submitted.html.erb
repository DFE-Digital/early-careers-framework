<% what_changes_submitted_partial = @setup_school_cohort_form.attributes[:what_changes_choice] + "_submitted" %>

<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <% academic_year = TabLabelDecorator.new(@cohort.description) %>
    <%= govuk_panel title_text: "You've submitted your training information", text: "Academic year #{academic_year}", classes: "govuk-!-margin-bottom-8" %>

    <%= render partial: what_changes_submitted_partial %>

    <%= govuk_link_to "Return to manage your training", schools_dashboard_path, class: "govuk-link govuk-link--no-visited-state" %>
  </div>
</div>
